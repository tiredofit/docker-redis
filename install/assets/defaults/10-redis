#!/command/with-contenv bash

DATA_PATH=${DATA_PATH:-"/data/db/"}
ENABLE_PERSISTENCE=${ENABLE_PERSISTENCE:-"TRUE"}
ENABLE_SOCKET=${ENABLE_SOCKET:-"TRUE"}
LOG_FILE=${LOG_FILE:-"redis.log"}
LOG_LEVEL=${LOG_LEVEL:-"notice"}
LOG_PATH=${LOG_PATH:-"/logs/"}
REDIS_PASS=${REDIS_PASS:-}
REDIS_PORT=${REDIS_PORT:-"6379"}
SOCKET_FILE=${SOCKET_FILE:-"redis.sock"}
SOCKET_PATH=${SOCKET_PATH:-"/var/run/redis/"}
SOCKET_PERMISSIONS=${SOCKET_PERMISSIONS:-"777"}